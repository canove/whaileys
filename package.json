    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^27.0.6",
    "jimp": "^0.16.1",
    "link-preview-js": "^2.1.13",
    "prettier": "^3.6.2",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.30.5",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.8.1",
    "typedoc": "^0.22.0",
    "typescript": "^4.0.0"
  }
}
