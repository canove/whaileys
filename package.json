{
  "name": "whaileys",
  "version": "6.2.1",
  "description": "WhatsApp API",
  "homepage": "https://github.com/canove/whaileys",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "whatsapp",
    "js-whatsapp",
    "whatsapp-api",
    "whatsapp-web",
    "whatsapp",
    "whatsapp-chat",
    "whatsapp-group",
